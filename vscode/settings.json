{
  "vim.hlsearch" : true,
  "workbench.colorTheme" : "Monokai ST3",
  "editor.fontFamily" : "Hack",
  "vim.leader" : "<space>",
  "snyk.yesWelcomeNotification" : false,
  "editor.insertSpaces" : false,
  "vim.useCtrlKeys" : true,
  "javascript.updateImportsOnFileMove.enabled" : "always",
  "files.associations" : {
    "*.html" : "html",
    "*.symlink": "shellscript",
    "*config.symlink": "ssh_config"
  },
  "editor.wordWrap" : "off",
  "workbench.settings.useSplitJSON" : true,
  "window.zoomLevel" : 1,
  "editor.inlineSuggest.enabled" : true,
  "editor.minimap.enabled" : false,
  "vim.easymotion" : true,
  "cSpell.userWords" : [
    "Chiedo",
    "Sparkbox",
    "autoprefixer",
    "circleci",
    "endfor",
    "flexbox",
    "htaccess",
    "mixins",
    "subpackage",
    "tabindex"
  ],
  "editor.tabSize" : 2,
  "editor.accessibilitySupport" : "off",
  "terminal.integrated.env.osx" : {
    "FIG_NEW_SESSION": "1"
  },
  "workbench.settings.editor" : "json",
  "vsintellicode.modify.editor.suggestSelection" : "automaticallyOverrodeDefaultValue",
  "workbench.startupEditor" : "newUntitledFile",
  "editor.codeActionsOnSave" : {
    "source.fixAll" : true
  },
  "vim.insertModeKeyBindings" : [
    {
      "after" : [
        "<Esc>"
      ],
      "before" : [
        "j",
        "j"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive" : [
    {
      "after" : [
        "d",
        "d"
      ],
      "before" : [
        "<leader>",
        "d"
      ]
    },
    {
      "commands" : [
        ":nohl"
      ],
      "before" : [
        "<C-n>"
      ]
    }
  ],
  "eslint.validate" : [
    "typescript",
    "typescriptreact"
  ],
  "editor.fontSize" : 14,
  "typescript.updateImportsOnFileMove.enabled" : "always",
  "vim.incsearch" : true,
  "vim.handleKeys" : {
    "<C-a>" : false,
    "<C-f>" : false
  },
  "editor.lineNumbers" : "on",
  "vim.useSystemClipboard" : true,
  "explorer.confirmDelete" : false,
  "editor.multiCursorModifier" : "ctrlCmd",
  "editor.suggestSelection" : "first",
  "intelephense.stubs" : [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
    "Core",
    "ctype",
    "curl",
    "date",
    "dba",
    "dom",
    "enchant",
    "exif",
    "FFI",
    "fileinfo",
    "filter",
    "fpm",
    "ftp",
    "gd",
    "gettext",
    "gmp",
    "hash",
    "iconv",
    "imap",
    "intl",
    "json",
    "ldap",
    "libxml",
    "mbstring",
    "meta",
    "mysqli",
    "oci8",
    "odbc",
    "openssl",
    "pcntl",
    "pcre",
    "PDO",
    "pdo_ibm",
    "pdo_mysql",
    "pdo_pgsql",
    "pdo_sqlite",
    "pgsql",
    "Phar",
    "posix",
    "pspell",
    "readline",
    "Reflection",
    "session",
    "shmop",
    "SimpleXML",
    "snmp",
    "soap",
    "sockets",
    "sodium",
    "SPL",
    "sqlite3",
    "standard",
    "superglobals",
    "sysvmsg",
    "sysvsem",
    "sysvshm",
    "tidy",
    "tokenizer",
    "xml",
    "xmlreader",
    "xmlrpc",
    "xmlwriter",
    "xsl",
    "Zend OPcache",
    "zip",
    "zlib",
    "wordpress"
  ],
  "files.trimTrailingWhitespace" : true,
  "editor.wordSeparators" : "\/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "files.exclude" : {
    "**\/.git" : false
  },
  "editor.cursorStyle" : "line",
  "editor.autoClosingBrackets" : "never",
  "svg.preview.mode" : "svg",
  "workbench.settings.openDefaultSettings": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
